#!/bin/bash
# Script to run Python code stored in the $PYCODE environment variable

# Check if PYCODE is set; if not, display an error and exit
if [ -z "$PYCODE" ]; then
    echo "Error: Environment variable PYCODE is not set."
    exit 1
fi

# Run the Python code stored in PYCODE using python3
python3 -c "$PYCODE"
